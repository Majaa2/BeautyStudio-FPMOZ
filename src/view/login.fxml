<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Login">
   <children>
      <Label layoutX="48.0" layoutY="87.0" text="Korisničko ime" />
      <Label layoutX="47.0" layoutY="192.0" text="Lozinka" />
      <TextField fx:id="korisnikTxt" layoutX="46.0" layoutY="134.0" prefHeight="25.0" prefWidth="508.0" promptText="Unesite korisničko ime" />
      <PasswordField fx:id="lozinkaTxt" layoutX="46.0" layoutY="232.0" prefHeight="25.0" prefWidth="508.0" promptText="Unesite lozinku" />
      <Button fx:id="prijavaBtn" layoutX="471.0" layoutY="301.0" mnemonicParsing="false" onAction="#loginUser" text="Prijava" />
      <Button fx:id="registracijaBtn" layoutX="331.0" layoutY="20.0" mnemonicParsing="false" onAction="#registrationUser" prefHeight="25.0" prefWidth="98.0" text="Registracija" />
      <Button fx:id="uslugeBtn" layoutX="449.0" layoutY="20.0" mnemonicParsing="false" onAction="#pregledUsluga" text="Pregled usluga" />
   </children>
</AnchorPane>
